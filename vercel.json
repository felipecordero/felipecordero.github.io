{
  "buildCommand": "./build.sh",
  "outputDirectory": "public",
  "framework": "hugo",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "routes": [
    {
      "src": "/index.xml",
      "headers": {
        "Content-Type": "application/rss+xml"
      }
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
